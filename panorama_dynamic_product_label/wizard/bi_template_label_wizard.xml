<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="bi_template_label_wizard_form_view" model="ir.ui.view">
			<field name="name">bi.template.label.wizard.form.view</field>
			<field name="model">bi.template.label.wizard</field>
			<field name="arch" type="xml">
				<form string="Dynamic Product Label">

					<group string="Label">
						<field name="qty"/>
						<field name="product_template_ids" widget="many2many_tags"/>
					</group>

					<footer>
						<button name="generate_label" string="Print"
						        type="object" class="oe_highlight"/>
						or
						<button string="Cancel" class="oe_link" special="cancel"/>
					</footer>
				</form>
			</field>
		</record>

		<act_window id="action_open_generate_label_wizard_tem" key2="client_print_multi"
		            name="Dynamic Product Label"
		            res_model="bi.template.label.wizard"
		            src_model="product.template" view_mode="form" target="new" view_type="form"/>
	</data>

</odoo>