<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend" inherit_id="website.assets_frontend" priority="1">
        <xpath expr="//link[last()]" position="after">
            <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet"/>

            <link rel='stylesheet' href="/theme_syncoria_electronics/static/src/css/font-awesome.min.css"/>
            <link rel='stylesheet' href="/theme_syncoria_electronics/static/src/css/jquery.mCustomScrollbar.min.css"/>
            <link rel='stylesheet' href="/theme_syncoria_electronics/static/src/css/slick.min.css"/>
            <link rel='stylesheet' href="/theme_syncoria_electronics/static/src/scss/theme_style_var.scss"/>
            <link rel='stylesheet' href="/theme_syncoria_electronics/static/src/scss/theme_style.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_syncoria_electronics/static/src/js/jquery.mCustomScrollbar.concat.min.js"></script>
            <script src="/theme_syncoria_electronics/static/src/js/slick.min.js" type="text/javascript"></script>
            <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-zoom/1.7.21/jquery.zoom.min.js"/>
<!--            <script type="text/javascript" src="/theme_syncoria_electronics/static/lib/js/infinite-scroll/infinite-scroll.pkgd.min.js"></script>-->
            <script type="text/javascript" src="/theme_syncoria_electronics/static/src/js/custom.js"></script>
        </xpath>
    </template>
    <template id="snippet_bestsellers_options_assets_editor" inherit_id="website.assets_editor" name="snippet_bestsellers_options_assets_editor">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/theme_syncoria_electronics/static/src/js/seller_snippet_options.js" />
        </xpath>
    </template>
    <template id="syncoria_assets_editor" inherit_id="web_editor.assets_editor" name="Web Editor Assets (used in private and public editor)">        
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_syncoria_electronics/static/src/js/editor.js"/>
        </xpath>
    </template>
</odoo>
